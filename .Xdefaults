yeahconsole*term:urxvt
yeahconsole*toggleKey:None+F2
yeahconsole*keyFull:None+F11
yeahconsole*keySmaller:Control+m
yeahconsole*keyBigger:Control+n
yeahconsole*consoleHeight:40
yeahconsole*xOffset:380
yeahconsole*screenWidth:1800
yeahconsole*restart:1
yeahconsole*handleWidth:0
yeahconsole*stepSize:0
yeahconsole*scrollBar:false
URxvt*saveLines: 12000

URxvt*background: #1d2021
URxvt*foreground: #ebdbb2

URxvt*font: xft:Ubuntu Mono:pixelsize=16:antialias=true:hinting=true
URxvt*boldFont: xft:Ubuntu Mono:bold:pixelsize=16:antialias=true:hinting=true

URxvt*externalBorder: 2
URxvt*internalBorder: 2
URxvt*borderLess: false

URxvt.url-launcher:     /usr/bin/chromium-browser
URxvt.matcher.button:   1

URxvt*cutchars:           BACKSLASH '"'&()*,;<=>?@[]^{|}

